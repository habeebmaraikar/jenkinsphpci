<?xml version="1.0" encoding="utf-8" ?>
<phpdox xmlns="http://xml.phpdox.net/config">
 <project name="Math" source="../src" workdir="phpdox">
  <collector publiconly="false" backend="parser" encoding="auto">
   <include mask="*.php" />
   <exclude mask="" />
  </collector>

  <generator output="api">
   <enrich base="logs">
    <source type="build" />
    <source type="git" />
    <source type="phploc" />
    <source type="checkstyle" />
    <source type="pmd" />
    <source type="phpunit">
        <coverage path="../coverage/xml" />
    </source>
   </enrich>

   <build engine="html" enabled="true" output="api">
    <file extension="html" />
   </build>

  </generator>
 </project>
</phpdox>
